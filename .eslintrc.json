{
  "env": {
    "es2021": true,
    "browser": true
  },
  "extends": ["plugin:prettier/recommended"],
  "ecmaVersion": 12,
  "plugins": ["prettier"],
  "rules": {
    "no-undef": 0,
    "no-undefined": 0,
    "no-unused-vars": 0,
    "prettier/prettier": "error",
    "semi": ["error", "never"]
  }
}
